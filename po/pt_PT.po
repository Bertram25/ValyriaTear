#: ../dat/battles/tutorial_battle_dialogs.lua:70
#: ../dat/battles/tutorial_battle_dialogs.lua:97 ../dat/config/quests.lua:82
#: ../dat/maps/layna_forest/layna_forest_cave1_2.lua:517
#: ../dat/maps/layna_forest/layna_forest_wolf_cave.lua:379
